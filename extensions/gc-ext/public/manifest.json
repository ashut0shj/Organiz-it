{
  "manifest_version": 3,
  "name": "Link Extractor",
  "version": "1.0",
  "description": "Extracts the URL of the current tab.",
  "permissions": ["tabs", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon100.png"
    }
  }
}
